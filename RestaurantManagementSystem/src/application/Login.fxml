<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginController">
   <children>
      <Button layoutX="174.0" layoutY="213.0" mnemonicParsing="false" onAction="#Login" text="Login" />
      <PasswordField fx:id="txtPassword" layoutX="117.0" layoutY="161.0" promptText="password" />
      <TextField fx:id="txtUsername" layoutX="117.0" layoutY="119.0" promptText="username" />
      <Label layoutX="14.0" layoutY="352.0" prefHeight="17.0" prefWidth="110.0" text="Login status:" />
      <Label layoutX="14.0" layoutY="369.0" prefHeight="17.0" prefWidth="110.0" text="Database status:" />
      <Label layoutX="169.0" layoutY="43.0" text="Welcome!" />
      <Label fx:id="lblLoginStatus" alignment="CENTER_RIGHT" layoutX="124.0" layoutY="352.0" prefHeight="17.0" prefWidth="262.0" text="Waiting" textAlignment="RIGHT" textFill="#f50707" />
      <Label fx:id="lblDatabaseStatus" alignment="CENTER_RIGHT" layoutX="124.0" layoutY="369.0" prefHeight="17.0" prefWidth="262.0" text="Waiting" textAlignment="RIGHT" textFill="#22da11" />
   </children>
</AnchorPane>
